Host *
AddKeysToAgent yes
UseKeychain yes
ControlMaster auto
ControlPath ~/.ssh/master-%r@%h:%p
ControlPersist 8h
Compression yes
ForwardX11Trusted yes
ForwardAgent yes
HashKnownHosts no

host github.com
  ControlMaster no
  ControlPersist no

Host *.proteus.co !remote.proteus.sh
  ProxyCommand ssh -qW %h:%p remote.proteus.sh 
  ServerAliveInterval 60 
  ForwardAgent yes

Host *.proteus.sh !remote.proteus.sh
  ProxyCommand ssh -qW %h:%p remote.proteus.sh 
  ServerAliveInterval 60 
  ForwardAgent yes

Host *.pfapp.co
  ProxyCommand ssh -qW %h:%p remote.proteus.sh 
  ServerAliveInterval 60 
  ForwardAgent yes

Host obi-wan.i2rd.com
  ControlMaster no


